{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "alperenersoy/filament-export": "^0.2.4",
        "bensampo/laravel-enum": "^6.1",
        "bezhansalleh/filament-shield": "^2.3",
        "bradyrenting/filament-passwordless": "^1.0",
        "codeat3/blade-codicons": "^1.27",
        "codeat3/blade-iconpark": "^1.4",
        "davidhsianturi/blade-bootstrap-icons": "^1.3",
        "filament/filament": "^2.0",
        "guzzlehttp/guzzle": "^7.5",
        "laravel/framework": "^10.0",
        "laravel/tinker": "^2.8",
        "maatwebsite/excel": "^3.1",
        "stancl/tenancy": "^3.5",
        "swalbrun/filament-regex-import": "^0.2.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.10",
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^3.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^7.0",
        "pestphp/pest": "^2.3",
        "pestphp/pest-plugin-livewire": "^2.0",
        "phpmd/phpmd": "^2.10",
        "squizlabs/php_codesniffer": "^3.7",
        "vimeo/psalm": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "__Tests__\\": "tests/__test__/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan filament:upgrade"
        ],
        "test": [
            "@php vendor/bin/pest --configuration phpunit.xml"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "jeremykenedy/laravel-users"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
